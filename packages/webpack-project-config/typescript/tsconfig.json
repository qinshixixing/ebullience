{
  "extends": "@tsconfig/recommended/tsconfig.json",
  "display": "WebWebpack",
  "compilerOptions": {
    "module": "esnext",
    "target": "esnext",
    "allowSyntheticDefaultImports": true,
    "allowJs": true,
    "removeComments": true,
    "jsx": "react",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "noImplicitAny": false,
    "noEmit": true,
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "isolatedModules": true
  }
}
